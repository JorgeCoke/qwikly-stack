{
  "name": "super-qwik-template",
  "description": "Delightful, overpowered, beautifully handcrafted web framework template, built on top of Qwik, seasoned with modern tools.",
  "engines": {
    "node": ">=20.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "wrangler pages publish ./dist",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "serve": "wrangler pages dev ./dist",
    "start": "vite --open --mode ssr",
    "qwik": "qwik",
    "db:reset": "rm main.db && touch main.db && npm run migrate",
    "migrate": "npx tsx migrator.ts"
  },
  "devDependencies": {
    "@builder.io/qwik": "1.2.5",
    "@builder.io/qwik-city": "1.2.5",
    "@types/better-sqlite3": "^7.6.4",
    "@types/eslint": "8.40.2",
    "@types/node": "^20.4.0",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "5.61.0",
    "autoprefixer": "^10.4.14",
    "eslint": "8.44.0",
    "eslint-plugin-qwik": "1.2.5",
    "postcss": "^8.4.23",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "tailwindcss": "^3.3.1",
    "typescript": "5.1.6",
    "undici": "5.22.1",
    "vite": "4.4.0",
    "vite-tsconfig-paths": "4.2.0"
  },
  "dependencies": {
    "@lucia-auth/adapter-sqlite": "^2.0.0",
    "@modular-forms/qwik": "0.18.1",
    "@qwikest/icons": "^0.0.8",
    "better-sqlite3": "^8.5.0",
    "kysely": "^0.26.1",
    "lucia": "^2.0.0",
    "tailwind-merge": "^1.14.0"
  }
}
